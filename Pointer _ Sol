#include <bits/stdc++.h>
using namespace std;

int update(int a,int b) {
    int add = a + b;  
    return add;
}
int update1(int a, int b){
    int sub = a - b;
    if(sub<0){
        sub = sub*(-1);
    }
    return sub;
}
int main() {
    int a, b;
    cin >> a ;
    cin >> b;
    int ans = update(a, b);
    cout<< ans <<endl;
    int ans1 = update1(a, b);
    cout<<ans1;
    return 0;
}
